<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">ark.cn-beijing.volces.com</domain>
        <domain includeSubdomains="true">api.minimaxi.com</domain>
        <domain includeSubdomains="true">api.sensenova.cn</domain>
    </domain-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system"/>
        </trust-anchors>
    </base-config>
</network-security-config> 